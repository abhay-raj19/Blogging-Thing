<%- include('partials/header'); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" name="blog" placeholder="Enter your text.." required>
        </div>
        <div class="form-group">
        <label>Post</label>
        <textarea name="postbody" class="form-control" cols="30" rows="5"></textarea>
        </div>
        <button type="submit" class="btn btn-primary" id="" name="">Publish</button>
    </form>
<%- include('partials/footer'); -%>